module "{{fqn}}" {
    source = "{{ module_source }}"
    env  = "{{ env }}"
    profile = "{{ profile }}"
    fqn = "{{ fqn }}"
    workload = "{{ workload }}"
    cmd_id = "{{ cmd_id }}"
    eventrule_name  = "{{ params["eventrule_name"] }}"
    eventbus_name   = "{{ params["eventbus_name"] }}"
    event_pattern   = "{{ params["event_pattern"] }}"
    role_arn   = "{{ params["role_arn"] }}"
    #custom_target_arn = module.{{ profile }}_aws_sqs_queue_{{ params['target_name'] }}.arn
    custom_target_arn  = "{{ params["target_name"] }}"
    tags = {
    {% for k, v in  params['tags'].items() -%}
        "{{ k }}" = "{{ v }}"
    {% endfor -%}
    }
}
